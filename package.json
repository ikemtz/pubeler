{
  "name": "pubeler",
  "version": "1.0.5",
  "description": "A console application that can call Rest APIs with data from flat files.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pubeler": "./dist/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "tsc && node ./dist/index.js",
    "debug": "tsc && node --inspect-brk=9222 ./dist/index.js",
    "build": "tsc",
    "test": "tsc && npm i -g . && pubeler",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikemtz/pubeler.git"
  },
  "keywords": [
    "Excel",
    "CSV",
    "Rest",
    "Auth0",
    "OAuth2",
    "OIDC"
  ],
  "author": "@IkeMtz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikemtz/pubeler/issues"
  },
  "homepage": "https://github.com/ikemtz/pubeler#readme",
  "dependencies": {
    "axios": "^1.2.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "csv-parse": "^5.3.3",
    "fs": "0.0.1-security",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "typescript": "^4.9.3"
  }
}
